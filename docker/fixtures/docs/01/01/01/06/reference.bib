@inproceedings{Kerninon2009,
abstract = {The metal layered manufacturing processes produce net shape metal components by fusion of metal alloys in powder or wire form. The field of those processes has grown in past years and now offers a great potential. On the one hand, reliability of powder bed processes has been improved, allowing the manufacture of fully dense parts with good accuracy. On the other hand, other approaches have been developed like direct laser metallic powder deposition or weld deposition-based rapid manufacturing. These processes allow multi-axis approaches for direct manufacturing. With these approaches, it is possible to build non-planar layers on a complex substrate. In this paper, different possibilities to manufacture direct part are analyzed. Different ways to choose substrate and the different processes used are explored. Based on this analysis, a methodology with seven steps is proposed and the first step of this method, which is a DFM approach that consists in creating customized substrate based on a CAD of the part, is presented.},
address = {Leiria, Portugal},
author = {Kerninon, J and Mognol, P and Hasco\"{e}t, J.Y.},
booktitle = {4th International Conference on Advanced Research in Virtual and Rapid Prototyping},
file = {:C$\backslash$:/Documents and Settings/ponche/Mes documents/bibliographie/Global approach to design and manufacture Direct Parts. 2009.pdf:pdf},
pages = {111--116},
title = {{Global approach to design and manufacture Direct Parts}},
year = {2009}
}
@inproceedings{Mognol2005a,
abstract = {This paper deals with the integration of the environmental aspects (energy consumption) in Rapid Prototyping. It is important to minimize the manufacturing time but there is no general rule for optimization of electrical energy consumption. Consequently, each RP system must be tested with energy consumption considerations. Our objective is to take into account the complete life-cycle of a rapid prototyped part. Various manufacturing parameters have been tested on three rapid prototyping systems. The objective is to select sets of parameters for reduction of electrical energy consumption. As a result, some rules were proposed to minimize this electrical energy consumption during a job.},
address = {Grenoble (France)},
author = {Mognol, Pascal and Perry, Nicolas and Lepicart, Denis},
booktitle = {12th CIRP Life Cycle Engineering},
file = {:C$\backslash$:/Documents and Settings/ponche/Mes documents/bibliographie/Environment aspect of rapid prototyping process energy consumption. 2005.pdf:pdf},
keywords = {electrical power,energy,rapid prototyping},
title = {{Environment aspect of rapid prototyping: process energy consumption}},
year = {2005}
}
