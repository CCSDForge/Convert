@inproceedings{Kerninon2008a,
abstract = {Metal deposition processing is well known for cladding by laser or welding process. Nevertheless, these systems are not optimized for direct manufacturing. One of the main lock is the need of dedicated CAD/CAM systems to slice and generate path strategies. Research at the IRCCYN laboratory (France) focus on the impact of cladding or welding head programming on dimensional properties and microstructure of the manufactured parts. In this context, path strategies are studied and different strategies are proposed and compared. This paper presents results for welding path strategies. The integration of deposition head on a 6 axis robot system was used to improve slicing strategies and parts.},
address = {Austin, Texas (USA)},
author = {Kerninon, Julien and Mognol, Pascal and Hascoet, Jean-Yves and Legonidec, Christian},
booktitle = {Solid Freeform Fabrication Symposium},
file = {:C$\backslash$:/Documents and Settings/ponche/Mes documents/bibliographie/Effect of path strategies on metallic parts manufactured by additive process. 2008.pdf:pdf},
pages = {352--361},
title = {{Effect of path strategies on metallic parts manufactured by additive process}},
year = {2008}
}
@inproceedings{Kerbrat2009a,
abstract = {Dans le contexte actuel de concurrence internationale, la diversit\'{e} des produits propos\'{e}s et leur complexit\'{e} croissante imposent aux industriels de d\'{e}velopper des produits innovants, de qualit\'{e}, \`{a} moindre co\^{u}t, et dans diff\'{e}rentes variantes permettant de satisfaire les exigences des clients. Le march\'{e} de la fabrication d’outillages (moules et matrices) a donc fortement \'{e}volu\'{e} et les outillages doivent d\'{e}sormais \^{e}tre con\c{c}us et mis au point plus rapidement, \^{e}tre adaptables aux diverses variantes de produits et fabriqu\'{e}s \`{a} co\^{u}t ma\^{\i}tris\'{e}. Pour cela, une double approche – modulaire et hybride - est propos\'{e}e. Ainsi l’outillage est vu comme un puzzle 3D, compos\'{e} de modules r\'{e}alis\'{e}s s\'{e}par\'{e}ment avec le proc\'{e}d\'{e} de fabrication le plus adapt\'{e} (usinage grande vitesse ou fabrication par ajout de mati\`{e}re) puis assembl\'{e}s. Une m\'{e}thodologie de conception et de fabrication d’outillages modulaires hybrides est donc d\'{e}velopp\'{e}e. Il s’agit dans un premier temps de cartographier les zones de l’outillage les plus complexes \`{a} fabriquer, \`{a} partir du calcul d’indices de fabricabilit\'{e}. Dans cette \'{e}tape, le mod\`{e}le CAO de l’outillage est d\'{e}compos\'{e} en cellules \'{e}l\'{e}mentaires (d\'{e}composition en octree), et les valeurs des indices de fabricabilit\'{e} sont calcul\'{e}es automatiquement pour chaque cellule. Le but est de disposer d’une vision pr\'{e}cise de la complexit\'{e} de fabrication afin de d\'{e}finir, une conception modulaire hybride de l’outillage qui r\'{e}duise les difficult\'{e}s de fabrication.},
address = {La Plagne, France},
author = {Kerbrat, Olivier and Mognol, Pascal and Hasco\"{e}t, Jean-Yves},
booktitle = {11\`{e}me Colloque National AIP Primeca},
file = {:C$\backslash$:/Documents and Settings/ponche/Mes documents/bibliographie/M\'{e}thode de conception d'outillages modulaires hybrides bas\'{e}e sur l'\'{e}valuation quantitative de la complexit\'{e} de fabrication. 2009.pdf:pdf},
keywords = {conception,fabrication rapide,hybride,octree,usinage},
organization = {Institut de Recherche en Communications et Cybern\'{e}tique de Nantes},
pages = {1--13},
title = {{M\'{e}thode de conception d'outillages modulaires hybrides bas\'{e}e sur l'\'{e}valuation quantitative de la complexit\'{e} de fabrication}},
year = {2009}
}
