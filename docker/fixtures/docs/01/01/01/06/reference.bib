@inproceedings{Kerbrat2007,
abstract = {In the modern mass production industry (automobile, electric household\ldots) the design and manufacturing of dies and moulds are probably the most time and money-consuming operations. Thus, tools have to be designed, manufactured and developed at lower cost in the shortest lead-time. In this context, a methodology to optimize the manufacturing of tools, with hybrid and modular design points of view (instead of monobloc tools) is proposed. In fact, to deal with local modifications and diversifying alternatives of prod- ucts, only one or few components would have to be re-manufactured and replaced. Different manufacturing processes (additive and subtractive) are compared to choose the most suitable ones in order to optimize the overall tool manufacturing cost and lead-time. The aim is to obtain a hybrid modular decomposition of the tool that can reduce manufacturing difficulties.},
address = {Leiria, Portugal},
author = {Kerbrat, Olivier and Mognol, Pascal and Hascoet, Jean-Yves},
booktitle = {3rd International Conference on Advanced Research in Virtual and Rapid Prototyping},
file = {:C$\backslash$:/Documents and Settings/ponche/Mes documents/bibliographie/Manufacturing criteria in hybrid modular tools how to combine additive and subtractive processes. 2007.pdf:pdf},
organization = {IRCCyN, MO2P},
pages = {419--424},
title = {{Manufacturing criteria in hybrid modular tools : how to combine additive and subtractive processes}},
year = {2007}
}
